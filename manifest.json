{
    "manifest_version": 3,
    "name": "Website Activity Logger",
    "version": "1.0",
    "description": "Logs website visits and sends data to an API.",
    "permissions": [
      "tabs",
      "storage",
      "alarms",
      "activeTab"
    ],
    "host_permissions": [
      "https://uez62rtr20.execute-api.us-east-1.amazonaws.com/"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "hello_extensions.png",
        "48": "hello_extensions.png",
        "128": "hello_extensions.png"
      }
    }
}